
@{
    ViewData["Title"] = "Home Page";
    string message = (string)ViewBag.LogoutMessage;
    
}
@if ( !(message is null) )
{

<div id="notificationLogout" >
    
        <div class="alert alert-info alert-dismissible fade show" role="alert">
            @message
            <button type="button" class="close" data-dismiss="alert" aria-label="Close">
                <span aria-hidden="true">&times;</span>
            </button>
        </div>
    
</div>
}
<div class="text-center">
    <h1 class="display-4">Password Manager App</h1>
   
   

    </div>




@section Scripts {

<script src="~/js/MyJsFunctions.js" asp-append-version="true"></script>

}



